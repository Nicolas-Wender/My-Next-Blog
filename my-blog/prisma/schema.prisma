generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id  Int  @id  @default(autoincrement())
  title  String  @db.VarChar(60)
  desc  String  @db.VarChar(250)
  content  String
  img  String
  date  DateTime  @default(now())
  authorId  Int
  author  String
}